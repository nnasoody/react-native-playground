{
    "rules": {
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "react/prop-types": 1,
        "react/no-multi-comp": 1,
        "no-unused-vars": 1,
        "no-console": 0
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "globals": {
        "$STATE": 1,
        "Stripe": 1,
        "$": 1,
        "analytics": 1,
        "$TRACKER": 1,
        "$CONTEXT": 1,
        "$UPLOADER": 1
    },
    "extends": "eslint:recommended",
    "ecmaFeatures": {
        "modules": true,
        "jsx": true,
        "experimentalObjectRestSpread": true
    },
    "plugins": [
    ]
}
